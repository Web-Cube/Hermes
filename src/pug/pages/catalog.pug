extends ../layouts/base

append vars
	- title = 'Catalog';

block content
	.wrap
		+b.breadcrumbs
			+e.UL.list.flex._center
				+e.LI.item
					+e.A.link(href="#") Главная
				+e.LI.item Каталог
	+b.SECTION.catalog
		+e.wra.wrap._big
			+e.H1.title.h4 Каталог сумок Hermes
			+e.filter.filter__
				+e.row.flex
					+e.column
						+e.select.select__.js-select
							input.js-select-input(name="model" value="" type="hidden")
							+e.head.js-select-head
								+e.title
									+e.SPAN.text Модель: 
									+e.SPAN.label.js-select-label Birkin
								+e.arrow
									+icon('dropDown')
							+e.body._big
								+e.check-list.flex
									+e.LABEL.checkbox.checkbox__
										+e.INPUT.input(type="checkbox" checked)
										+e.icon
										+e.text Birkin
									+e.LABEL.checkbox.checkbox__
										+e.INPUT.input(type="checkbox")
										+e.icon
										+e.text Kelly
									+e.LABEL.checkbox.checkbox__
										+e.INPUT.input(type="checkbox")
										+e.icon
										+e.text Evelyne
									+e.LABEL.checkbox.checkbox__
										+e.INPUT.input(type="checkbox")

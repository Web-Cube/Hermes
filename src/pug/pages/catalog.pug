										+e.text Birkin
									+e.LABEL.checkbox.checkbox__
										+e.INPUT.input(type="checkbox")
										+e.icon
										+e.text Kelly
					+e.column
						+e.select.select__.js-select
							input.js-select-input(name="status" value="" type="hidden")
							+e.head.js-select-head
								+e.title
									+e.SPAN.text Cостояние: 
									+e.SPAN.label.js-select-label б/у
								+e.arrow
									+icon('dropDown')
							+e.body._big
								+e.check-list.flex
									+e.LABEL.checkbox.checkbox__.js-select-item(data-value="б/у")
										+e.INPUT.input(type="radio" name="status" checked)
										+e.icon
										+e.text б/у
									+e.LABEL.checkbox.checkbox__.js-select-item(data-value="новое")
										+e.INPUT.input(type="radio" name="status")
										+e.icon
										+e.text новое
					+e.column
						+e.select.select__.js-select
							input.js-select-input(name="material" value="" type="hidden")
							+e.head.js-select-head
								+e.title
									+e.SPAN.text Материал: 
									+e.SPAN.label.js-select-label кожа крокодила
								+e.arrow
									+icon('dropDown')
							+e.body._big
								+e.check-list.flex
									+e.LABEL.checkbox.checkbox__
										+e.INPUT.input(type="checkbox" checked)
										+e.icon
										+e.text кожа крокодила
									+e.LABEL.checkbox.checkbox__
										+e.INPUT.input(type="checkbox")
										+e.icon
										+e.text кожа слона
					+e.column
						+e.select.select__.js-select
							input.js-select-input(name="size" value="" type="hidden")
							+e.head.js-select-head
								+e.title
									+e.SPAN.text Размер: 
									+e.SPAN.label.js-select-label 20-50
									span  см
								+e.arrow
									+icon('dropDown')
							+e.body._big
								+e.range-slider.range-slider__.js-calc(data-min="20" data-max="50")
									.ui-slider-handle
										+e.text
											span.js-calc-from 20
											span  см
									.ui-slider-handle
										+e.text
											span.js-calc-to 50
											span  см
					+e.column
						+e.select.select__.js-select
							input.js-select-input(name="model" value="" type="hidden")
							+e.head.js-select-head
								+e.title
									+e.SPAN.text Цена: 
									+e.SPAN.label.js-select-label по убыванию
								+e.arrow
									+icon('dropDown')
							+e.body._big
								+e.check-list.flex
									+e.LABEL.checkbox.checkbox__.js-select-item(data-value="по убыванию")
										+e.INPUT.input(type="radio" name="price" checked)
										+e.icon
										+e.text по убыванию
									+e.LABEL.checkbox.checkbox__.js-select-item(data-value="по возрастанию")
										+e.INPUT.input(type="radio" name="price")
										+e.icon
										+e.text по возрастанию

			+e.list.flex
				+e.item.item-product__
